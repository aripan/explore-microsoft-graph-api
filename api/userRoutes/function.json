{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"],
      "route": "userRoutes/{endpoint}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "direction": "in",
      "name": "teamsfxContext",
      "type": "TeamsFx"
    }
  ],
  "scriptFile": "../dist/userRoutes/index.js"
}
